{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaRoleReadAccess",
      "Effect": "Allow",
      "Principal": { "AWS": "*" },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::pi-logs-bucket-xxxxxxxxx/*",
      "Condition": {
        "StringEquals": {
          "aws:PrincipalArn": "arn:aws:iam::XXXXXXXXXXXXX:role/Lambda-pihole-analyzer"
        }
      }
    }
  ]
}
